{
  "name": "redis-semaphore",
  "version": "3.2.1",
  "description": "Distributed mutex and semaphore based on Redis",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "test": "eslint --ext .js,.ts . && mocha",
    "coveralls": "nyc mocha && nyc report --reporter=text-lcov | coveralls",
    "dev": "mocha -w",
    "check-types": "tsc -b tsconfig.check-types.json",
    "build": "yarn build-commonjs",
    "build-commonjs": "rm -rf lib && yarn tsc -b tsconfig.build-commonjs.json",
    "build-es": "rm -rf es && yarn tsc -b tsconfig.build-es.json",
    "preversion": "yarn test && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:swarthy/redis-semaphore.git"
  },
  "keywords": [
    "redis",
    "mutex",
    "semaphore"
  ],
  "author": "Alexander Mochalin (horroshow@mail.ru)",
  "license": "MIT",
  "devDependencies": {
    "@swarthy/wait-for": "^2.0.3",
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/debug": "^4.1.5",
    "@types/ioredis": "^4.17.10",
    "@types/mocha": "^8.2.0",
    "@types/node": "14.14.20",
    "@types/sinon": "^9.0.10",
    "@types/sinon-chai": "^3.2.5",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "4.12.0",
    "@typescript-eslint/parser": "4.12.0",
    "babel-eslint": "^10.1.0",
    "benchmark": "^2.1.4",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "coveralls": "^3.1.0",
    "eslint": "7.17.0",
    "eslint-config-inclusive": "1.2.10",
    "eslint-plugin-node": "11.1.0",
    "ioredis": "^4.19.4",
    "mocha": "8.2.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.1.0",
    "sinon": "9.2.2",
    "sinon-chai": "3.5.0",
    "snyk": "1.437.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">= 10.0.0"
  },
  "peerDependencies": {
    "ioredis": "^4.1.0"
  },
  "dependencies": {
    "debug": "^4.3.1",
    "uuid": "^8.3.2"
  }
}
